{
  "manifest_version": 3,
  "name": "SimpleSearchNewTab",
  "version": "2.1",
  "description": "A minimalist Chrome extension that replaces the default new tab page with a sleek search interface.",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "web_accessible_resources": [{
    "resources": [
      "svgs/engines/*.svg"
    ],
    "matches": ["<all_urls>"]
  }],
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  } 
}
